<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="true">
        <shape>
            <corners android:radius="@dimen/tagcloud_corner"/>
            <solid android:color="@color/tagcloud_item_text_unchecked"/>
        </shape>
    </item>
    <item android:state_activated="false">
        <shape>
            <corners android:radius="@dimen/tagcloud_corner"/>
            <solid android:color="@color/tagcloud_item_text_checked"/>
        </shape>
    </item>
</selector>
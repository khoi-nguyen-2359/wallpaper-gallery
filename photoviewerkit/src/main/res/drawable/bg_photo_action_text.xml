<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="@color/bg_photo_action_text_pressed"/>
			<corners android:radius="5dp"/>
			<stroke android:width="1dp" android:color="@color/photo_action_bg_stroke"/>
		</shape>
	</item>
	<item>
		<shape>
			<solid android:color="@color/bg_photo_action_text_normal"/>
			<corners android:radius="5dp"/>
			<stroke android:width="1dp" android:color="@color/photo_action_bg_stroke"/>
		</shape>
	</item>
</selector>